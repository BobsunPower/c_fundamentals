using System;

namespace x02.PrintNumbersInReverseOrder
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int l = int.Parse(Console.ReadLine());
            int[] n = new int[l];
            for (int i = 0; i < l; i++) {n[i] = int.Parse(Console.ReadLine());}
            for (int i = n.Length - 1; i > -1; i--) {Console.Write($"{n[i]} ");}
        }
    }
}
